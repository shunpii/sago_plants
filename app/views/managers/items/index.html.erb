<div class="item-list">
  <h1>商品一覧</h1>
  <% @items.each do |item| %>
    <table>
      <tr>
        <th>商品名</th>
        <td><%= link_to item.name, managers_item_path(item.id) %></td>
      </tr>
      <tr>
        <th>価格</th>
        <td><%= item.price %></td>
      </tr>
    </table>
  <% end %>
</div>

